{
  "name": "NewRelic auto-refresh buster",
  "description": "disable NR's default auto-refresh behavior to save some $",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://one.newrelic.com/*"],
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "16": "./icons/moneybag_16.png",
    "32": "./icons/moneybag_32.png",
    "64": "./icons/moneybag_64.png",
    "128": "./icons/moneybag_128.png"
  }
}
